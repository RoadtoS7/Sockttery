<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d5bea6"
    android:id="@+id/layoutMain"
    tools:context=".ui.MainActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/snow1"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow2"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="84dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow7"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow9"
            android:layout_width="38dp"
            android:layout_height="32dp"
            android:layout_marginStart="156dp"
            android:layout_marginTop="32dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow10"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="256dp"
            android:layout_marginTop="196dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow8"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="160dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow5"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="136dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow6"
            android:layout_width="32dp"
            android:layout_height="39dp"
            android:layout_marginStart="348dp"
            android:layout_marginTop="184dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow09"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginStart="324dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow3"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginStart="280dp"
            android:layout_marginTop="136dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/snow4"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="252dp"
            android:src="@drawable/ic_snow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="314dp"
            android:layout_height="431dp"
            android:src="@drawable/tree_btn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/img_sock_right_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="68dp"
            android:layout_marginEnd="72dp"
            android:src="@drawable/group_5_copy_7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <ImageView
            android:id="@+id/img_sock_right_mid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:src="@drawable/group_5_copy_12"
            app:layout_constraintEnd_toStartOf="@+id/img_sock_right_top"
            app:layout_constraintHorizontal_bias="0.45"
            app:layout_constraintStart_toEndOf="@+id/img_sock_left_top"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <ImageView
            android:id="@+id/img_sock_right_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="28dp"
            android:src="@drawable/group_5_copy_8"
            app:layout_constraintEnd_toStartOf="@+id/img_sock_left_bottom"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/img_sock_left_mid"
            app:layout_constraintTop_toBottomOf="@+id/img_sock_right_mid" />

        <ImageView
            android:id="@+id/img_sock_left_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/group_5_copy_10"
            app:layout_constraintStart_toEndOf="@+id/img_sock_right_mid"
            app:layout_constraintTop_toBottomOf="@+id/img_sock_right_top" />

        <ImageView
            android:id="@+id/img_mypage"
            android:layout_width="wrap_content"
            android:layout_height="104dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="80dp"
            android:src="@drawable/my_page_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/img_sock_left_mid" />

        <ImageView
            android:id="@+id/img_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="60dp"
            android:src="@drawable/money_btn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/img_mypage" />

        <ImageView
            android:id="@+id/img_sock_left_mid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="24dp"
            android:src="@drawable/group_5_copy_11"
            app:layout_constraintEnd_toStartOf="@+id/img_sock_right_mid"
            app:layout_constraintTop_toBottomOf="@+id/img_sock_left_top" />

        <ImageView
            android:id="@+id/img_sock_left_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="84dp"
            android:layout_marginTop="84dp"
            android:src="@drawable/group_5_copy_9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView3" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/christmas_bold"
            android:text="My Page"
            android:textColor="#ffffff"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/img_mypage"
            app:layout_constraintEnd_toEndOf="@+id/img_mypage"
            app:layout_constraintHorizontal_bias="0.600"
            app:layout_constraintStart_toStartOf="@+id/img_mypage"
            app:layout_constraintTop_toTopOf="@+id/img_mypage" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14,000"
            android:textColor="#ffffff"
            android:textSize="30sp"
            android:fontFamily="@font/christmas_bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_money"
            app:layout_constraintEnd_toEndOf="@+id/img_money"
            app:layout_constraintStart_toStartOf="@+id/img_money"
            app:layout_constraintTop_toTopOf="@+id/img_money" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.154"
            app:srcCompat="@drawable/dollar_copy_6" />

        <TextView
            android:id="@+id/mainTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="52dp"
            android:fontFamily="@font/rredokaone_regular"
            android:shadowColor="#4c000000"
            android:shadowDy="2"
            android:shadowRadius="4"
            android:text="TOUCH SOCKS"
            android:textColor="#ffffff"
            android:textSize="40sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</RelativeLayout>